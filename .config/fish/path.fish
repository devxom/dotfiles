# yarn binary
fish_add_path -a $HOME/.yarn/bin

# yarn global modules (hack for me)
fish_add_path -a $HOME/.config/yarn/global/node_modules/.bin

# `code` binary from VS Code
fish_add_path -a /Applications/Visual Studio Code.app/Contents/Resources/app/bin

# Homebrew binary
fish_add_path -a /opt/homebrew/bin